n    name : \"loginPage\",\n    data() {\n        return {\n            email : '',\n            password : '',\n            \n        }\n    },\n    methods: {\n        login () {\n            let payload = {\n                email : this.email,\n                password : this.password\n            }\n\n            axios({\n                url: '/login',\n                method: \"POST\",\n                data : payload\n            })\n            .then( ({data}) => {\n                // console.log(data.access_token)\n                console.log(data)\n                localStorage.setItem('access_token', data.access_token)\n                localStorage.setItem('id', data.id)\n                localStorage.setItem('name', data.name)\n                localStorage.setItem( 'organization', data.organization)\n                this.$emit('loginSubmit') \n            })\n            .catch( err => {\n                console.log(err)\n            })\n        },\n        register () {\n            this.$emit('register')\n        },\n        signIn () {\n            this.$emit('signin')\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>\n"},"lineCount":null}},"error":null,"hash":"372b7ee5976bc9a218a4987bc49d1e66","cacheData":{"env":{}}}